{
  "name": "keycap-studio",
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "release": "npm run build && powershell -Command \"Compress-Archive -Path 'dist/*' -DestinationPath 'releases/KeycapStudio-v%npm_package_version%.zip' -Force\" && gh release create v%npm_package_version% releases/KeycapStudio-v%npm_package_version%.zip --title \"KeycapStudio v%npm_package_version%\" --notes-file releases/RELEASES.md"
  },
  "dependencies": {
    "@react-three/drei": "^9.95.0",
    "@react-three/fiber": "^8.15.0",
    "leva": "^0.9.35",
    "maath": "^0.10.7",
    "opentype.js": "^1.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.1",
    "three-csg-ts": "^3.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.16",
    "jsdom": "^28.1.0",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
